
@{
    ViewBag.Title = "Select Contract";
    Layout = null;
}
<div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
        <div class="modal-header" style="padding: 15px 25px !important;">
            <h5 class="modal-title" id="exampleModalLabel">@ViewBag.Title</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body" style="padding: 10px 10px !important;">

            <div class="form-group">

                <div class="col-md-12">
                    <div class="row">

                        <div class="col-lg-12">
                            <form>
                                <div>
                                    <input type="radio" name="radio" checked="checked" value="buyer" id="buyer" class="RadioButton">
                                    <label for="seller">For Buyer</label>
                                </div>
                                <div>
                                    <input type="radio" name="radio" value="seller" id="seller" class="RadioButton">

                                    <label for="seller"> For Seller</label>
                                </div>
                                <div>
                                    <input type="radio" name="radio" value="complete" id="complete" class="RadioButton">

                                    <label for="complete">Complete</label>
                                </div>
                                <div id="log"></div>
                            </form>

                        </div>

                    </div>

                </div>
            </div>

        </div>
        <div class="modal-footer" style="padding: 10px 10px !important;">
            <button type="button" class="btn btn-success" onclick="OK('@Url.Action("Print","IndentDomestic",new{id=@Session["ID"].ToString(), Area="Indent"})')">
                <span>
                    <i class="fa fa-save"></i>
                    <span>OK</span>
                </span>
            </button>
            <button type="button" class="btn btn-warning" data-dismiss="modal">
                <span>
                    <i class="flaticon-cancel"></i>
                    <span>Cancel</span>
                </span>
            </button>
        </div>
    </div>
</div>

<script src="~/Content/VigorJs/IndDomestic/IndDomestic.js"></script>


