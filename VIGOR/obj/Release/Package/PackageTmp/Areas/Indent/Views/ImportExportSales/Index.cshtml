                            <div class="row">
                                <div class="col-sm-12">
                                    <table class="table table-striped- table-bordered table-hover table-checkable dataTable no-footer dtr-inline" id="m_table_1" role="grid" aria-describedby="kt_table_1_info" style="width: 992px;">
                                        <thead style="background-color:#28469c; color:#ffffff">
                                            <tr role="row">


                                                <th>Inquiry No.</th>
                                                <th>Date</th>
                                                <th>Customer</th>
                                                <th>Program</th>
                                                <th>Commodity</th>
                                                <th>Quantity</th>
                                                <th>UOS</th>
                                                <th>Contract</th>
                                                <th>Closed</th>
                                                <th>Attachments</th>

                                            </tr>
                                        </thead>

                                        <tbody>

                                            <tr role="row" class="odd">
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                                <td></td>
                                            </tr>
                                        </tbody>

                                    </table>
                                </div>
                            </div>
